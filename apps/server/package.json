{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"node -r esbuild-register ./index.ts\" -e .ts",
    "start": "node -r esbuild-register ./index.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "tsc --noEmit && TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.12",
    "@types/supertest": "^2.0.11",
    "config": "*",
    "esbuild": "^0.14.14",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.5"
  }
}
