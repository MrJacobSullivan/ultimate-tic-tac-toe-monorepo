{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"node -r esbuild-register ./index.ts\" -e .ts",
    "start": "node -r esbuild-register ./index.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "tsc --noEmit && TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "engine": "*",
    "express": "^4.17.2",
    "glob": "^7.2.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "socket-controllers": "^0.0.5",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.12",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "config": "*",
    "esbuild": "^0.14.14",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
